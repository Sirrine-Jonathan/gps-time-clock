{
  "expo": {
    "name": "gps-time-clock",
    "description": "A time clock application for companies with remote employees.",
    "slug": "gps-time-clock",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.9.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/Sirrine-Jonathan/gps-time-clock",
    "android": {
      "package": "com.gps.time",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDx0nB7pwmDQ3psBGTcB-znD58wg6PQM28"
        }
      }
    }
  }
}